@using ConceptosMVC.Models
@model List<Empleado>
@{
                /**/

                ViewBag.Title = "MostrarEmpleados";
}

<h2>Mostrar Empleados Sesisión</h2>

@if (Session["EMPLEADOS"] != null)
{
    <table class="table table-bordered">
        <tr>
            <th>ID</th>
            <th>Apellido</th>
            <th>Oficio</th>
            <th>Salario</th>

        </tr>
        @foreach (Empleado e in Model)
        {
            <tr>
                <td>@e.IdEmpleado</td>
                <td>@e.Apellido</td>
                <td>@e.Oficio</td>
                <td>@e.Salario</td>

            </tr>
        }
    </table>
    
}
else
{
    <h2>No hay empleados almacenados</h2>
}


@Html.ActionLink("Volver al listado de empleados", "AlmacenarEmpleados", "Empleados");